package gov.wisconsin.framework.security.base;

import gov.wisconsin.framework.security.cargo.UserLogin_Cargo;

public interface ISecurity {
	
	/**
	 * Returns authorization level of a user for specified service.
	 * Creation date: (04/12/2004 10:00:00 AM)
	 */
	public String getPageAuthForProfile(String prflID, String pageID);

	/**
	  * gets the user id for the wamsid
	  * Creation Date: Jul 7, 2004
	  */
	public UserLogin_Cargo getUIDforWIDfromAPP(String wid);
}
