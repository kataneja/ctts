package gov.wisconsin.timesheet.transport.helper;

public class TimesheetSummaryData {
	private String status = "";
	private String weekNumber = "";
	private long totalOverallHours = 0;
	private long totalBillableHours = 0;
	private long totalNonBillableHours = 0;
	
	public TimesheetSummaryData() {}
	
	public TimesheetSummaryData(String status, String weekNumber, long totalOverallHours, long totalBillableHours, long totalNonBillableHours) {
		super();
		this.status = status;
		this.weekNumber = weekNumber;
		this.totalOverallHours = totalOverallHours;
		this.totalBillableHours = totalBillableHours;
		this.totalNonBillableHours = totalNonBillableHours;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getWeekNumber() {
		return weekNumber;
	}

	public void setWeekNumber(String weekNumber) {
		this.weekNumber = weekNumber;
	}

	public long getTotalOverallHours() {
		return totalOverallHours;
	}

	public void setTotalOverallHours(long totalOverallHours) {
		this.totalOverallHours = totalOverallHours;
	}

	public long getTotalBillableHours() {
		return totalBillableHours;
	}

	public void setTotalBillableHours(long totalBillableHours) {
		this.totalBillableHours = totalBillableHours;
	}

	public long getTotalNonBillableHours() {
		return totalNonBillableHours;
	}

	public void setTotalNonBillableHours(long totalNonBillableHours) {
		this.totalNonBillableHours = totalNonBillableHours;
	}
}
